{
  "name": "@deltares/fews-web-oc-charts",
  "version": "3.0.0-alpha.0",
  "description": "FEWS Chart Library",
  "author": {
    "name": "Werner Kramer",
    "email": "werner.kramer@deltares.nl"
  },
  "keywords": [
    "FEWS",
    "Charts"
  ],
  "license": "MIT",
  "type": "module",
  "exports": {
    "import": "./lib/esm/index.js",
    "require": "./lib/cjs/index.js"
  },
  "main": "./lib/cjs/index.js",
  "module": "./lib/esm/index.js",
  "typings": "./lib/types/index.d.ts",
  "files": [
    "dist/*",
    "lib/*"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Deltares/fews-web-oc-charts"
  },
  "bugs": {
    "url": "https://github.com/Deltares/fews-web-oc-charts/issues"
  },
  "engines": {
    "node": ">=0.14"
  },
  "scripts": {
    "build": "npm run clean && npm run build:esm && npm run build:cjs && webpack",
    "build:esm": "tsc",
    "build:cjs": "tsc --module commonjs --outDir lib/cjs",
    "clean": "shx rm -rf dist",
    "lint": "eslint src/**.ts",
    "lint:fix": "eslint src/**.ts --fix",
    "test": "jest"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
  },
  "devDependencies": {
    "@types/d3": "^7.1.0",
    "@types/jest": "^27.0.0",
    "@types/lodash-es": "^4.17.6",
    "@types/luxon": "^3.3.0",
    "@types/node": "^14.0.20",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "colors": "^1.4.0",
    "copy-webpack-plugin": "^10.0.0",
    "eslint": "^8.3.0",
    "jest": "^29.5.0",
    "jest-junit": "^15.0.0",
    "jest-teamcity-reporter": "^0.9.0",
    "shx": "^0.3.2",
    "terser-webpack-plugin": "^5.2.5",
    "ts-jest": "^29.1.0",
    "typescript": "^4.5.0",
    "webpack": "^5.78.0",
    "webpack-cli": "^4.9.1"
  },
  "dependencies": {
    "csstype": "^3.0.0",
    "d3": "^7.1.1",
    "jest-environment-jsdom": "^29.5.0",
    "lodash-es": "^4.17.21",
    "luxon": "^3.3.0"
  }
}
