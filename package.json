{
  "name": "@deltares/fews-web-oc-charts",
  "version": "3.1.0",
  "description": "FEWS Chart Library",
  "author": {
    "name": "Werner Kramer",
    "email": "werner.kramer@deltares.nl"
  },
  "keywords": [
    "FEWS",
    "Charts"
  ],
  "license": "MIT",
  "type": "module",
  "exports": {
    "types": "./lib/types/index.d.ts",
    "import": "./lib/esm/index.js",
    "require": "./lib/cjs/index.js"
  },
  "main": "./lib/cjs/index.js",
  "module": "./lib/esm/index.js",
  "typings": "./lib/types/index.d.ts",
  "private": false,
  "files": [
    "dist/*",
    "lib/*"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Deltares/fews-web-oc-charts"
  },
  "bugs": {
    "url": "https://github.com/Deltares/fews-web-oc-charts/issues"
  },
  "engines": {
    "node": ">=22.0.0"
  },
  "scripts": {
    "build": "tsc && vite build",
    "clean": "shx rm -rf dist",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "test": "jest",
    "sonar": "sonar-scanner -Dsonar.host.url=$SONAR_URL -Dsonar.login=$SONAR_TOKEN -Dsonar.projectKey=$SONAR_KEY -Dsonar.projectName='Delft-FEWS Web OC Charts'"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
  },
  "devDependencies": {
    "@eslint/js": "^9.19.0",
    "@rollup/plugin-typescript": "^12.1.2",
    "@types/d3": "^7.4.0",
    "@types/jest": "^27.0.0",
    "@types/lodash-es": "^4.17.6",
    "@types/luxon": "^3.3.3",
    "@types/node": "^22.10.10",
    "colors": "^1.4.0",
    "eslint": "^9.19.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-junit": "^15.0.0",
    "jest-teamcity-reporter": "^0.9.0",
    "sass": "^1.69.5",
    "shx": "^0.3.2",
    "sonarqube-scanner": "^3.0.1",
    "typescript-eslint": "^8.21.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.3",
    "vite": "^6.0.11"
  },
  "dependencies": {
    "csstype": "^3.0.0",
    "d3": "^7.8.5",
    "lodash-es": "^4.17.21",
    "luxon": "^3.4.3"
  }
}
