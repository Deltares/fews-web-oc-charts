{
  "name": "wb-charts",
  "version": "1.1.0",
  "description": "Chart Library",
  "keywords": [],
  "main": "./dist/wb-charts.umd.js",
  "typings": "./dist/types/wb-charts.d.ts",
  "files": [
    "dist/*",
    "src/*"
  ],
  "author": "Werner Kramer <werner.kramer@vortech.nl>",
  "repository": {
    "type": "git",
    "url": ""
  },
  "license": "MIT",
  "engines": {
    "node": ">=6.0.0"
  },
  "scripts": {
    "build": "npm run clean && tsc && webpack",
    "clean": "shx rm -rf dist",
    "lint": "eslint src/**.ts",
    "lint:fix": "eslint src/**.ts --fix",
    "prepare": "npm run build",
    "test": "jest"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/test/"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 95,
        "lines": 95,
        "statements": 95
      }
    },
    "collectCoverage": true
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
  },
  "devDependencies": {
    "@types/d3": "^7.1.0",
    "@types/jest": "^26.0.15",
    "@types/lodash": "^4.14.157",
    "@types/luxon": "^2.0.7",
    "@types/node": "^14.0.20",
    "@typescript-eslint/eslint-plugin": "^2.18.0",
    "@typescript-eslint/parser": "^2.18.0",
    "colors": "^1.4.0",
    "copy-webpack-plugin": "^6.3.0",
    "jest": "^26.1.0",
    "shx": "^0.3.2",
    "terser-webpack-plugin": "^4.2.3",
    "ts-jest": "^26.1.1",
    "ts-loader": "^6.2.1",
    "typedoc": "^0.17.8",
    "typescript": "^4.5.0",
    "webpack": "^4.42.0",
    "webpack-cli": "^3.3.11"
  },
  "dependencies": {
    "csstype": "^3.0.0",
    "d3": "^7.1.1",
    "lodash": "^4.17.19",
    "luxon": "^2.1.1"
  }
}
